# Copy to .env and set values. Do not commit .env or real secrets.
# Serverless auth uses ES_API_KEY only.

ES_URL=
ES_API_KEY=
ES_VERIFY_TLS=true
ES_INDEX_PREFIX=pmai

# Optional: Kibana Agent Builder (Streamlit uses these for Narrator + Auditor)
KIBANA_URL=
KIBANA_API_KEY=
# If 404, try: KIBANA_SPACE_ID=default or your space id; or set KIBANA_CONVERSE_PATH to the full path that works (e.g. from curl).
# KIBANA_SPACE_ID=
# KIBANA_CONVERSE_PATH=
# Use exact agent ID from Kibana Agent Builder (URL or settings). If narrator 404s, check Narrator agent ID.
AGENT_NARRATOR_ID=incident-narrator-agent
AGENT_AUDITOR_ID=incident-integrity-auditor
AGENT_TIMEOUT_SECS=60
# Set to 1 to log each Agent Builder URL and response status (console) when debugging 404s
# AGENT_BUILDER_DEBUG=0
